module full_adder (
  input s_check,
  input cout_check,
	input x,
	input y,
	input cin,
	output s,
	output cout,
 output checker,
  ) {
sig i, j, k, cout_connector, s_connector; // connector
always {
	s_connector = x ^ y ^ cin;
	i = x & y;
	j = x & cin;
	k = y & cin;
	cout_connector = i | j | k;
 cout = cout_connector;
 s = s_connector;
    if (cout_connector==cout_check && s_connector==s_check) {
      checker = 1;
	  }
    else {
      checker = 0;  
    }
}
}
